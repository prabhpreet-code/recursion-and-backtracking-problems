#include <iostream>

using namespace std;

#include <bits/stdc++.h>

void stringRemover(char input[] , int start ){
    if(input[start] == '\0'){
        return ;
    }
    stringRemover(input, start+1) ;

    if(input[start] == 'x'){
        int len = strlen(input) ;
        int i ;
        for(i=start+1 ; i < len ; i++){
            input[i-1] = input[i] ;
        }
        input[i-1] = '\0' ;
    }
}

int main()
{
    char input[10000] ;
    cin.getline(input, 10000);

    stringRemover(input , 0);
    cout << input << endl;
}
